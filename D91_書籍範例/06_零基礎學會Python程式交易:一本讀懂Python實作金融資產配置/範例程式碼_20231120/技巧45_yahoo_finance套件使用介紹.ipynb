    "# -*- coding: utf-8 -*-\n",
    "# 載入套件\n",
    "import yfinance as yf\n",
    "\n",
    "# 商品名稱\n",
    "prod = \"^GSPC\"\n",
    "\n",
    "# 取得歷史資料\n",
    "data = yf.download(prod, start=\"2023-12-01\", end=\"2023-12-15\")\n",
    "\n",
    "# 取得全部資料\n",
    "data = yf.download(prod, period=\"max\")\n",
    "data"
   ]
